// prisma/schema.prisma

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Admin {
  id        BigInt   @id @default(autoincrement())
  email     String   @unique
  password  String
  creadoEn  DateTime @default(now())
}
